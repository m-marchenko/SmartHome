
@model ICollection<SmartHome.Models.ISensor>
@{
// ViewBag.Title = "Home Page";
}

<table class="table table-striped">
    <thead>
        <tr>
            <th>Параметр</th>
            <th>Значение</th>
            <th>Время</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var d in Model)
        {
            <tr>
                <td>@d.DisplayName</td>
                <td>@d.Value</td>
                <td>@d.MeasureTime.ToString("dd.MM HH:mm")</td>
            </tr>
        }
    </tbody>
</table>
